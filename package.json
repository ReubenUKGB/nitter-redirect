{
  "name": "privacy-redirect-plus",
  "description": "A lightweight tool that ensures requests to various platforms redirect to their privacy-focused alternative frontends.",
  "version": "1.0.0",
  "scripts": {
    "build": "zip -r privacy-redirect-plus.zip . -x \"./.git/*\" \"./node_modules/**\" \"./node_modules/*\" \"./_metadata/**\" \"./_metadata/*\" \"./img/screenshot.png\" \"./.gitignore\" \"./package.json\" \"./package-lock.json\"",
    "dev": "npx web-ext run --target=chromium",
    "clean": "rm privacy-redirect-plus.zip"
  },
  "devDependencies": {
    "web-ext": "^7.12.0"
  }
}
